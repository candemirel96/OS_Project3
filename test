BlockHeader* blk1;
  BlockHeader* blk2;
  BlockHeader* blk3;

  blk1 = firstBlock; 
  firstBlock->next = blk1;

  blk1->size = 16;
 

  blk2 = blk1 + blk1->size + 1;
  blk2->size = 32;
  
  blk3 = blk2 + blk2->size +1;
  blk3->size = 64;
  blk1->next = blk2;
  blk2->next = blk3;
  blk3->next = NULL;